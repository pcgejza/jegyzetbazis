{% if not is_granted("IS_AUTHENTICATED_REMEMBERED") %}
        <form {#action="{{ path('modded_fos_registration') }}" {{ form_enctype(form) }} method="POST" autocomplete="off" class="fos_user_registration_register"#}>
		<div class="formLine">
			<div class="labelText">{{ form_label(form.name) }}</div>
			<div class="inputs">
				{{ form_widget(form.name) }}
			</div>
		</div>
	</form>
{% endif %}
