<div class="miniBox">
    <div class="miniBoxTitle">{{title}}</div>
    <div class="miniBoxContent">
        {% if Elements|length > 0 %}
        <table>
            {% for Element in Elements %}
            <tr>
                <td title="{{Element['originalName']}}">
                    <a href="{{Element['path']}}">{{Element['name']}}</a>
                </td>
                <th>{{Element['value']}}</th>
            </tr>
            {% endfor %}
        </table>
        {% else %}
            nincsenek elemek
        {% endif %}
    </div>
</div>